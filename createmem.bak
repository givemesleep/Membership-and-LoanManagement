require_once '../cruds/config.php';
$Masterlist = "SELECT 
                p.memberID AS ID, un.unID AS UniqueID,
                CONCAT(p.memSur,', ', p.memGiven, ' ', p.memMiddle, ' ', p.suffixes) AS Fullname,
                CONCAT(c.memaddr, ' ', c.region, ' ', c.city, ' ', c.brgy, ' ') AS Address,
                m.marDep AS Marital, g.genders AS Gender,  TIMESTAMPDIFF(YEAR, birth, CURDATE()) AS Age
            FROM tbperinfo p
            JOIN tbuninfo un ON p.memberID = un.memberID
            JOIN tbconinfo c ON p.memberID = c.memberID
            JOIN tbmaritals m ON p.maritID = m.maritID
            JOIN tbgenders g ON p.gendID = g.gendID
            WHERE p.memstatID = 1 AND p.isActive = 1";
$stmtMaster = $conn->prepare($Masterlist);
$stmtMaster->execute();
$resM = $stmtMaster->rowCount();
if($resM > 0){
    $i = 0;
    while($rowM = $stmtMaster->fetch()){
        $counting = $i;
        $a = $rowM['ID']; // Member ID
        $b = $rowM['UniqueID']; // Unique ID
        $c = $rowM['Fullname']; // Fullname
        $d = $rowM['Address']; // Address
        $e = $rowM['Marital']; // Marital 
        $f = $rowM['Gender']; // Gender
        $g = $rowM['Age']; // Age

